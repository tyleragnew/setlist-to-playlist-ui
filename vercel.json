{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/callback",
            "destination": "/index.html"
        },
        {
            "source": "/setlistMetadata",
            "destination": "/index.html"
        },
        {
            "source": "/playlist",
            "destination": "/index.html"
        }
    ]
}